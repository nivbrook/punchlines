package com.nivbrook.punchlines.payload;

import java.time.Instant;
import java.util.List;

public class SetupDetailedResponse {
	private Long id;
	private String text;
	private String category;
	private UserSummary createdBy;
	private Instant creationDateTime;
	private List<PunchlineResponse> punchlines;
	
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getText() {
		return text;
	}
	public void setText(String text) {
		this.text = text;
	}
	public String getCategory() {
		return category;
	}
	public void setCategory(String category) {
		this.category = category;
	}
	public UserSummary getCreatedBy() {
		return createdBy;
	}
	public void setCreatedBy(UserSummary createdBy) {
		this.createdBy = createdBy;
	}
	public Instant getCreationDateTime() {
		return creationDateTime;
	}
	public void setCreationDateTime(Instant creationDateTime) {
		this.creationDateTime = creationDateTime;
	}
	public List<PunchlineResponse> getPunchlines() {
		return punchlines;
	}
	public void setPunchlines(List<PunchlineResponse> punchlines) {
		this.punchlines = punchlines;
	}
	
	
}
